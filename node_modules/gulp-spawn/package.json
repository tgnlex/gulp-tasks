{
  "name": "gulp-spawn",
  "description": "spawn plugin for gulp",
  "version": "2.0.0",
  "homepage": "https://github.com/pioug/gulp-spawn",
  "repository": {
    "type": "git",
    "url": "git://github.com/pioug/gulp-spawn.git"
  },
